    "@types/jest": "29.5.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "29.6.1",
    "eslint": "8.45.0",
    "eslint-plugin-svelte3": "4.0.0",
    "fa-svelte": "3.1.0",
    "gh-pages": "5.0.0",
    "husky": "8.0.3",
    "jest": "29.6.1",
    "prettier": "3.0.0",
    "prettier-plugin-svelte": "3.0.0",
    "sass": "1.64.0",
    "svelte": "3.59.2",
    "svelte-check": "2.9.1",
    "svelte-markdown": "0.2.3",
