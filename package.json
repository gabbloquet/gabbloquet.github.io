    "@types/jest": "29.1.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "29.1.2",
    "eslint": "8.25.0",
    "eslint-plugin-svelte3": "4.0.0",
    "fa-svelte": "3.1.0",
    "gh-pages": "4.0.0",
    "husky": "8.0.1",
    "jest": "29.1.2",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "2.7.1",
    "sass": "1.55.0",
    "svelte": "3.50.1",
    "svelte-check": "2.9.1",
    "svelte-markdown": "0.2.3",
    "svelte-preprocess": "4.10.7",
    "svelte-scrollto": "0.2.0",
    "svelte-spa-router": "3.3.0",
