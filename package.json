    "format": "npx prettier --write \"{,!(node_modules|public)/**/}*.{js,svelte}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/preset-env": "7.19.3",
    "@fortawesome/free-solid-svg-icons": "6.2.0",
    "@sveltejs/vite-plugin-svelte": "1.0.9",
