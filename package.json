{
  "homepage": "https://gabbloquet.github.io/",
  "name": "gabbloquet.github.io",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "jest src -- --watch",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "fix": "npx eslint --fix \"{,!(node_modules|public|dist)/**/}*.{js,svelte}\"",
    "format": "npx prettier --write \"{,!(node_modules|public|dist)/**/}*.{js,svelte}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.22.15",
    "@babel/preset-env": "7.23.2",
    "@fortawesome/free-solid-svg-icons": "6.4.0",
    "@sveltejs/vite-plugin-svelte": "2.4.6",
    "@tsconfig/svelte": "5.0.2",
    "@types/jest": "29.5.7",
    "babel-jest": "29.7.0",
    "eslint": "8.52.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-svelte": "2.34.0",
    "fa-svelte": "3.1.0",
    "gh-pages": "6.0.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "prettier": "3.0.3",
    "prettier-plugin-svelte": "3.0.3",
    "sass": "1.69.5",
    "svelte": "4.2.2",
    "svelte-check": "3.5.2",
    "svelte-eslint-parser": "^0.33.1",
    "svelte-markdown": "0.4.0",
    "svelte-preprocess": "5.0.4",
    "svelte-scrollto": "0.2.0",
    "svelte-spa-router": "3.3.0",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "vite": "4.5.0"
  },
  "dependencies": {
    "typed.js": "^2.1.0"
  }
}
