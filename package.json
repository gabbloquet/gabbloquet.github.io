{
  "homepage": "https://gabbloquet.github.io/",
  "name": "gabbloquet.github.io",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
